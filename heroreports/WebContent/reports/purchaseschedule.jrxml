	<queryString>
		<![CDATA[SELECT B.`prhdr_id`,DATE(prhdr_recv_date)prhdr_recv_date,
`prhdr_bill_no`,purchase_code,`purchase_refer_no`,CONCAT('Rs ',FORMAT(`prhdr_grand_total_amt`,2))prhdr_grand_total_amt,
COALESCE(`prhdr_paid_amount`,0)prhdr_paid_amount,
(`prhdr_grand_total_amt` - (COALESCE(`prhdr_paid_amount`,0)))tobe_paid,CONCAT('Rs.',FORMAT(`prhdr_discount_amt`,2))prhdr_discount_amt,
`prhdr_discount_type`,`prhdr_discount_value`,CONCAT('Rs ',FORMAT(`prhdr_shipping_cost`,2))prhdr_shipping_cost,
prhdr_paid_status,(`supplier_first_name`)suppliername,`address`,`city`,`state`,`zipcode`,`tin_number`,
`country_id`,`purchase_date`,`prhdr_tax_amt`,CONCAT('Rs ',FORMAT(`prhdr_total_amt`,2))prhdr_total_amt,
 number_to_words(prhdr_grand_total_amt) words,DATE(`pp_payment_time`)invdate,COALESCE(`supplier_invoice_no`,' ') invno,`terms_descriptions`terms
  FROM `hero_stock_purchase` A
LEFT JOIN `hero_stock_purchase_receive_hdr` B ON `purchase_code` = `pur_req_id`
 LEFT JOIN `hero_stock_supplier` S ON S.`supplier_id` = A.`supplier_id`
 LEFT JOIN `hero_stock_purchase_payment` Z ON Z.`prhdr_id`= B.`prhdr_id`
LEFT JOIN `hero_terms_conditions` d ON d.`terms_id`=B.`prhdr_terms`
  WHERE  `prhdr_bill_no` = $P{P_BILL_NO} LIMIT 1]]>
