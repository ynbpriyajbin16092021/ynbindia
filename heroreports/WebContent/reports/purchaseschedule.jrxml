	<subDataset name="itemdataset" uuid="426636d3-608b-4c52-ad0b-88e5b7013115">
		<parameter name="P_SUB_PURCHASE_ID" class="java.lang.String"/>
		<parameter name="P_SUB_BILLNO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT A.`pur_req_id`,`prec_product_id`,`product_code`,`product_name`,C.`hsncode`,`prec_recving_quantity`fesfs,
CASE WHEN (COALESCE(`prec_recving_quantity`,0)) = '0' THEN `pur_quantity` ELSE `prec_recving_quantity` END AS `prec_recving_quantity`,
COALESCE(`prec_free_count`,0)`prec_free_count`,COALESCE(`prec_batchno`,0)`prec_batchno`,
COALESCE(CONCAT(FORMAT(`prec_pur_price`,2)),'-')prec_pur_price,COALESCE(DATE_FORMAT(`prec_expiry_date`,'%d/%m/%Y'),'-') AS prec_expiry_date,
CONCAT(`prec_tax_per`,'% ')prec_taxper,`prod_tax_total` taxtotal,CONCAT(B.`sgst`,'% ')sgst,
COALESCE(CONCAT(FORMAT((`prec_pur_price` * `prec_recving_quantity`),2)),'-')taxablevalue,
COALESCE(CONCAT(FORMAT(D.`TAX_RATE`,0),'%' ),'-')centraltax,
COALESCE(CONCAT(FORMAT(E.`TAX_RATE`,0),'%' ) ,0)Statetax,
COALESCE(CONCAT(FORMAT(((`prec_pur_price` * `prec_recving_quantity`*prec_tax_per/100)),2)),'-')centraltaxamount,
COALESCE(CONCAT(FORMAT(((`prec_pur_price` * `prec_recving_quantity`*B.sgst/100)),2)),'-')statetaxamount,
CONCAT(FORMAT(((`prec_pur_price` * `prec_recving_quantity`*prec_tax_per/100)+(`prec_pur_price` * `prec_recving_quantity`*B.sgst/100)),2))totaltaxamount,
COALESCE(CONCAT(FORMAT(((`prec_pur_price` * `prec_recving_quantity`)+`prod_tax_total`),2)),'-')amount,`unit`unit,(D.`TAX_RATE`+E.`TAX_RATE`)twotaxpercent
FROM`hero_stock_purchase_request` F JOIN `hero_stock_purchase`ON `purchase_code`=F.`pur_req_id`
 LEFT JOIN `hero_stock_product` C ON C.`product_id`=F.`pur_product_id`
 LEFT JOIN `hero_stock_purchase_receive_hdr` A ON  A.`pur_req_id` =`purchase_code`
LEFT JOIN `hero_stock_purchase_received_dtl` B ON A.`prhdr_id` = B.`prec_hdr_id`  AND F.`pur_product_id` = B.`prec_product_id`
LEFT JOIN `hero_admin_tax` D ON D.`TAX_ID`=B.`prec_tax_per`
LEFT JOIN `hero_admin_tax` E ON E.`TAX_ID`=B.`sgst`
LEFT JOIN `hero_admin_unit_type` S ON S.`unit_type_id`=C.`unit_type_id`
WHERE  `purchase_id` =  $P{P_SUB_PURCHASE_ID} AND (`prec_recving_quantity` != 0 ) AND `prhdr_bill_no` = $P{P_SUB_BILLNO} AND (`prec_recving_quantity` != 0 )]]>
		</queryString>
